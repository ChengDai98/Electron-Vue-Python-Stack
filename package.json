{
  "name": "electron-vue-python-stack",
  "version": "1.0.0",
  "description": "Application cross-platform using Electron + Vue as GUI and Python with flask as Backend",
  "main": "main.js",
  "scripts": {
    "start": "set NODE_ENV=production && electron .",
    "watch": "set NODE_ENV=development && webpack --watch",
